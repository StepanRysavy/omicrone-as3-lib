package com.omicrone {		import com.omicrone.LogInput;		public class Log {		private static var log:Array = new Array;		private static var print:Boolean = true;		private static var externalOutput:Function = null;				public static function add (text:String, object:* = "~"):void {						var li:LogInput = new LogInput (text, object, print);						log.push(li);			if (externalOutput != null) externalOutput (li.toString());		}				public static function changePrint (b:Boolean):void {			print = b;		}				public static function setExternalOutput (fce:Function):void {			externalOutput = fce;		}	}}