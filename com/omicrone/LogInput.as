package com.omicrone {				public class LogInput {				private var _text:String;		private var _atObject:*;		private var _atTime:Date;				public function LogInput (text:String, object:* = null, print:Boolean = true) {			_text = text;			_atObject = object;			_atTime = new Date();						if (print) trace (this.toString());		}				public function toString():String {			var s:String;						s = createTime();			s += " ";			if (Commons.getClass(_atObject) == String) {				s += "[" + _atObject + "]";			} else {				s += Commons.getClass(_atObject)			}			s += ": " + _text						return s;		}				private function createTime ():String {			var s:String = "";						if (_atTime.getHours() < 10) {				s += "0" + _atTime.getHours();			} else {				s += _atTime.getHours();			}			s += ":";						if (_atTime.getMinutes() < 10) {				s += "0" + _atTime.getMinutes();			} else {				s += _atTime.getMinutes();			}			s += ":";						if (_atTime.getSeconds() < 10) {				s += "0" + _atTime.getSeconds();			} else {				s += _atTime.getSeconds();			}						return s;		}	}}